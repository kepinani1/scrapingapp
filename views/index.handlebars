<div>
    <h1 class="card z-depth-3 center-align">Get the Latest from 'Cosmopolitan'</h1>
    <div class="row">
      <div class="col s3"></div>
      <h5 class="card z-depth-3 center-align col s6">Click 'Scrape' to get the latest news and gossip around the world</h5>
    </div>
</div>

<div class="row">
  <div class="col s2"></div>
  <div class ="col s8 left-align">
    {{#each new}}
      <div class="card-panel">
        <h4>{{this.title}}</h4>
        {{!--Takes you to new.handlebars --}}
        <a href = "/readNews/{{this._id}}" id="btnSignUp">Read News and Leave Your Input</a>
      </div>
    {{/each}}
  </div>
</div>